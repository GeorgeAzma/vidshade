{
    "manifest_version": 3,
    "name": "Video Shader Filter",
    "description": "Applies a shader effect to the video",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "js": ["content.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }